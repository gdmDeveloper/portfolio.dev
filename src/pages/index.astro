---
import Badge from "../components/Badge.astro";
import Layout from "../layouts/Layout.astro";
import LinkedIn from "../icons/LinkedIn.astro";
import GitHub from "../icons/Github.astro";
import Gmail from "../icons/Gmail.astro";
import Briefcase from "../icons/Briefcase.astro";
import SocialPill from "../components/SocialPill.astro";
import SectionContainer from "../components/SectionContainer.astro";
import Experience from "../components/Experience.astro";
import Code from "../icons/Code.astro";
import Projects from "../components/Projects.astro";
---

<Layout title="Guillem Arana. Web Developer.",
description="Contrata a Guillem para crear tu aplicación web o móvil. Desarrollador web especializado en el front-end.">
	<main class="px-4">
		<SectionContainer class="py-44 pb-4 w-100 h-screen">
    <img class="rounded-full w-12 h-12 mb-4" src="data:image/jpeg;base64," alt="imagen guillem">
				<h1 class="text-white text-2xl lg:text-5xl font-bold flex flex-row gap-x-4 pb-6 lg:pb-8">
					Hola! Soy Guillem <div class="flex justify-center items-center "> <a href="https://www.linkedin.com/in/guillemarana-developer/"
						target="_blank"
						rel="noopener"
						class="flex justify-center items-center"><Badge>Open to work</Badge></a> </div>
			</h1>
			<h2 class="text-xl lg:text-2xl text-wrap max-w-[700px]">
				<span class="opacity-85">+3 años de experiencia.</span> <span class="text-yellow-100">Técnico superior en desarrollo de aplicaciones web.</span> <span class="text-blue-100">Alicante, España 🇪🇸. </span>
				Especializado en el desarrollo front-end.
			</h2>

			<nav class="flex flex-wrap gap-4 mt-5">
				<SocialPill href="https://www.linkedin.com/in/guillemarana-developer/">
					<LinkedIn class="size-4 md:size-6"/> 
						LinkedIn
				</SocialPill>

				<SocialPill href="https://github.com/gdmDeveloper/">
					<GitHub class="size-4 md:size-6"/> 
						GitHub
				</SocialPill>

				<SocialPill href="mailto:guillemarana21@gmail.com">
					<Gmail class="size-4 md:size-6"/> 
						guillemarana21@gmail.com
				</SocialPill>
				
			</nav>
		</SectionContainer>

		
		<SectionContainer id="experiencia" class="pt-24">
			<h2 class="text-3xl font-semiboldbold mb-8 flex gap-x-4 items-center">
				<Briefcase class="size-8"/>
				Experiencia laboral
			</h2>
			<Experience>
		</SectionContainer>

		<SectionContainer id="proyectos" class="pt-24">
			<h2 class="text-3xl font-semiboldbold mb-8 flex gap-x-4 items-center">
				<Code class="size-8"/>
				Proyectos
			</h2>
			<Projects>
		</SectionContainer>
		
	</main>
</Layout>
